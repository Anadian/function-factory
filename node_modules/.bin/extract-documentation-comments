#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cameron/dev/function-factory/node_modules/.pnpm/extract-documentation-comments@0.4.0/node_modules/extract-documentation-comments/source/node_modules:/home/cameron/dev/function-factory/node_modules/.pnpm/extract-documentation-comments@0.4.0/node_modules/extract-documentation-comments/node_modules:/home/cameron/dev/function-factory/node_modules/.pnpm/extract-documentation-comments@0.4.0/node_modules:/home/cameron/dev/function-factory/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cameron/dev/function-factory/node_modules/.pnpm/extract-documentation-comments@0.4.0/node_modules/extract-documentation-comments/source/node_modules:/home/cameron/dev/function-factory/node_modules/.pnpm/extract-documentation-comments@0.4.0/node_modules/extract-documentation-comments/node_modules:/home/cameron/dev/function-factory/node_modules/.pnpm/extract-documentation-comments@0.4.0/node_modules:/home/cameron/dev/function-factory/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../extract-documentation-comments/source/cli.js" "$@"
else
  exec node  "$basedir/../extract-documentation-comments/source/cli.js" "$@"
fi
