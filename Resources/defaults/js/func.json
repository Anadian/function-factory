{
	//"templatename": "Resources/templates/jsfunc.hbs",
	"template": "js func",
	"name": "FunctionName",
	"description": "Brief description.",
	"parametres": {
		"params": [
				{"type": "ArgumentType", "name": "parametre_name", "default": null, "description": "Brief parametre description."},
				{"type": "?Object", "name": "options", "default": { "value": "{}" }, "description": "[Reserved] Additional run-time options."}
		]
	},
	"return": { 
		"type": "ReturnType", 
		"description": "Return value description."
	},
	"throw": {
		"throws": [
			{ "type": "TypeError", "code": "ERR_INVALID_ARG_TYPE", "description": "Thrown if a given argument isn't of the correct type." }
		]
	},
	"asynchronous": false,
	"post_re": [
		{"search": ",\\)", "replace": " )", "flags": "gm"}
	]
}
